
@{
    ViewBag.Title = "Register";
}

<h2>Register</h2>

<div>
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">FirstName</div>
        <div><input type="text" name="firstName" class="form-control" /></div>
        <div class="form-group">LastName</div>
        <div><input type="text" name="lastName" class="form-control" /></div>
        <div class="form-group">Date of Birth</div>
        <div><input type="date" name="DOB" class="form-control" /></div>

        <div>@ViewBag.ErrorMessage</div>
        <button type="submit" class="btn-success">Submit Here</button>
</div>
    </form>
</div>